          <default>100</default>
        </key>
        <key name="latency-time" type="i">
          <default>10</default>
        </key>
        <key name="show-spectrum" type="b">
          <default>true</default>
        </key>
        <key name="spectrum-n-points" type="i">
          <default>200</default>
        </key>
        <key name="autovolume-state" type="b">
          <default>false</default>
        </key>
        <key name="autovolume-window" type="d">
          <default>2.00</default>
        </key>
        <key name="autovolume-target" type="i">
          <default>-12</default>
        </key>
        <key name="autovolume-tolerance" type="i">
          <default>1</default>
        </key>
        <key name="autovolume-threshold" type="i">
          <default>-50</default>
        </key>
        <key name="use-dark-theme" type="b">
          <default>false</default>
        </key>
    </schema>
</schemalist>
