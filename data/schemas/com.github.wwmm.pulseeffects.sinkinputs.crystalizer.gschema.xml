<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema
        id="com.github.wwmm.pulseeffects.sinkinputs.crystalizer"
        path="/com/github/wwmm/pulseeffects/sinkinputs/crystalizer/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="installed" type="b">
            <default>true</default>
        </key>
        <key name="post-messages" type="b">
            <default>true</default>
        </key>
        <key name="input-gain" type="d">
            <range min="-20.0" max="20.0"/>
            <default>0.0</default>
        </key>
        <key name="output-gain" type="d">
            <range min="-20.0" max="20.0"/>
            <default>0.0</default>
        </key>
        <key name="freq1" type="d">
            <range min="10" max="20000"/>
            <default>1000</default>
        </key>
        <key name="freq2" type="d">
            <range min="10" max="20000"/>
            <default>10000</default>
        </key>
        <key name="intensity-low" type="d">
            <range min="0" max="10"/>
            <default>1.0</default>
        </key>
        <key name="intensity-mid" type="d">
            <range min="0" max="10"/>
            <default>0.5</default>
        </key>
        <key name="intensity-high" type="d">
            <range min="0" max="10"/>
            <default>0.25</default>
        </key>
        <key name="mute-low" type="b">
            <default>false</default>
        </key>
        <key name="mute-mid" type="b">
            <default>false</default>
        </key>
        <key name="mute-high" type="b">
            <default>false</default>
        </key>
    </schema>
</schemalist>