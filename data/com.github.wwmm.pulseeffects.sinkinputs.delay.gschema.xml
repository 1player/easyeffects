<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="com.github.wwmm.pulseeffects.sinkinputs.delay" path="/com/github/wwmm/pulseeffects/sinkinputs/delay/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="post-messages" type="b">
            <default>false</default>
        </key>
        <key name="m-l" type="i">
            <range min="0" max="200"/>
            <default>0</default>
        </key>
        <key name="cm-l" type="d">
            <range min="0" max="100"/>
            <default>0</default>
        </key>
        <key name="m-r" type="i">
            <range min="0" max="200"/>
            <default>0</default>
        </key>
        <key name="cm-r" type="d">
            <range min="0" max="100"/>
            <default>0</default>
        </key>
        <key name="temperature" type="d">
            <range min="-60" max="60"/>
            <default>30</default>
        </key>
    </schema>
</schemalist>
