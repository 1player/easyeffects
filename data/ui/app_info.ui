<?xml version="1.0" encoding="UTF-8"?>
<interface domain="easyeffects">
    <object class="GtkBox" id="top_box">
        <property name="hexpand">1</property>
        <property name="vexpand">1</property>
        <property name="margin-start">6</property>
        <property name="margin-end">6</property>
        <property name="margin-top">6</property>
        <property name="margin-bottom">6</property>
        <property name="spacing">12</property>
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkBox">
                <property name="hexpand">1</property>
                <property name="vexpand">0</property>
                <property name="spacing">6</property>
                <child>
                    <object class="GtkSwitch" id="enable">
                        <property name="halign">start</property>
                        <property name="valign">center</property>
                        <property name="hexpand">0</property>
                        <property name="vexpand">0</property>
                    </object>
                </child>

                <child>
                    <object class="GtkImage" id="app_icon">
                        <property name="halign">start</property>
                        <property name="valign">center</property>
                        <property name="icon_size">large</property>
                    </object>
                </child>

                <child>
                    <object class="GtkBox">
                        <property name="hexpand">1</property>
                        <property name="vexpand">1</property>
                        <property name="halign">start</property>
                        <property name="valign">center</property>
                        <property name="spacing">6</property>
                        <property name="orientation">vertical</property>
                        <child>
                            <object class="GtkLabel" id="app_name">
                                <property name="halign">start</property>
                                <property name="valign">center</property>
                                <property name="label">App</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkLabel" id="media_name">
                                <property name="halign">start</property>
                                <property name="valign">center</property>
                                <property name="label">Description</property>
                                <style>
                                    <class name="dim-label" />
                                </style>
                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkCheckButton" id="blocklist">
                        <property name="halign">end</property>
                        <property name="valign">center</property>
                        <property name="label" translatable="yes">Add to Blocklist</property>
                    </object>
                </child>
            </object>
        </child>

        <child>
            <object class="GtkGrid">
                <property name="halign">center</property>
                <property name="valign">center</property>
                <property name="row-spacing">6</property>
                <property name="column-spacing">48</property>
                <child>
                    <object class="GtkLabel">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label" translatable="yes">Format</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="format">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label">f</property>
                        <style>
                            <class name="dim-label" />
                        </style>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object class="GtkLabel">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label" translatable="yes">Rate</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="rate">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label">r</property>
                        <style>
                            <class name="dim-label" />
                        </style>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object class="GtkLabel">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label" translatable="yes">Channels</property>
                        <layout>
                            <property name="column">2</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="channels">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label">c</property>
                        <style>
                            <class name="dim-label" />
                        </style>
                        <layout>
                            <property name="column">2</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object class="GtkLabel">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label" translatable="yes">Latency</property>
                        <layout>
                            <property name="column">3</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="latency">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label">l</property>
                        <style>
                            <class name="dim-label" />
                        </style>
                        <layout>
                            <property name="column">3</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object class="GtkLabel">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label" translatable="yes">State</property>
                        <layout>
                            <property name="column">4</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="state">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label">s</property>
                        <style>
                            <class name="dim-label" />
                        </style>
                        <layout>
                            <property name="column">4</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>

        <child>
            <object class="GtkBox">
                <property name="hexpand">1</property>
                <property name="vexpand">0</property>
                <property name="spacing">6</property>
                <child>
                    <object class="GtkToggleButton" id="mute">
                        <property name="halign">start</property>
                        <property name="valign">center</property>
                        <property name="icon_name">audio-volume-high-symbolic</property>
                    </object>
                </child>

                <child>
                    <object class="GtkScale" id="scale_volume">
                        <property name="hexpand">1</property>
                        <property name="valign">center</property>
                        <property name="adjustment">volume</property>
                        <property name="draw-value">1</property>
                        <property name="digits">0</property>
                        <property name="value-pos">bottom</property>
                        <marks>
                            <mark value="0" position="bottom"></mark>
                            <mark value="10" position="bottom"></mark>
                            <mark value="20" position="bottom"></mark>
                            <mark value="30" position="bottom"></mark>
                            <mark value="40" position="bottom"></mark>
                            <mark value="50" position="bottom"></mark>
                            <mark value="60" position="bottom"></mark>
                            <mark value="70" position="bottom"></mark>
                            <mark value="80" position="bottom"></mark>
                            <mark value="90" position="bottom"></mark>
                            <mark value="100" position="bottom"></mark>
                        </marks>
                    </object>
                </child>
            </object>
        </child>
    </object>

    <object class="GtkAdjustment" id="volume">
        <property name="lower">0</property>
        <property name="upper">100</property>
        <property name="step-increment">1</property>
        <property name="page-increment">10</property>
    </object>
</interface>