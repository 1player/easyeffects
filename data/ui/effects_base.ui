<?xml version="1.0" encoding="UTF-8"?>
<interface domain="pulseeffects">
    <object class="GtkBox" id="top_box">
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkStack" id="top_stack">
                <property name="hexpand">1</property>
                <property name="vexpand">1</property>
                <property name="transition_duration">250</property>
                <property name="transition_type">slide-left-right</property>
                <child>
                    <object class="GtkStackPage">
                        <property name="name">page_players</property>
                        <property name="title">Players</property>
                        <property name="child">
                            <object class="GtkBox">
                                <property name="orientation">vertical</property>
                                <property name="spacing">6</property>
                                <child>
                                    <object class="GtkScrolledWindow">
                                        <property name="hexpand">1</property>
                                        <property name="vexpand">1</property>
                                        <child>
                                            <object class="GtkViewport">
                                                <property name="hexpand">1</property>
                                                <property name="vexpand">1</property>
                                                <child>
                                                    <object class="GtkListView" id="listview_players">
                                                        <property name="hexpand">1</property>
                                                        <property name="vexpand">1</property>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </property>
                    </object>
                </child>

                <child>
                    <object class="GtkStackPage">
                        <property name="name">page_plugins</property>
                        <property name="title">Plugins</property>
                        <property name="child">
                            <object class="GtkLabel">
                                <property name="label">plugins</property>
                            </object>
                        </property>
                    </object>
                </child>
            </object>
        </child>

        <child>
            <object class="GtkHeaderBar">
                <property name="show_title_buttons">0</property>

                <property name="title-widget">
                    <object class="GtkStackSwitcher">
                        <property name="halign">center</property>
                        <property name="valign">end</property>
                        <property name="stack">top_stack</property>
                    </object>
                </property>

                <child>
                    <object class="GtkBox">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="spacing">3</property>
                        <child>
                            <object class="GtkImage" id="saturation_icon">
                                <property name="halign">center</property>
                                <property name="valign">center</property>
                                <property name="visible">0</property>
                                <property name="icon_name">dialog-warning-symbolic</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkLabel" id="global_output_level_left">
                                <property name="halign">center</property>
                                <property name="valign">center</property>
                                <property name="label">0</property>
                                <style>
                                    <class name="dim-label" />
                                </style>
                            </object>
                        </child>
                        <child>
                            <object class="GtkLabel" id="global_output_level_right">
                                <property name="halign">center</property>
                                <property name="valign">center</property>
                                <property name="label">0</property>
                                <style>
                                    <class name="dim-label" />
                                </style>
                            </object>
                        </child>
                        <child>
                            <object class="GtkLabel">
                                <property name="halign">center</property>
                                <property name="valign">center</property>
                                <property name="label">dB</property>
                                <style>
                                    <class name="dim-label" />
                                </style>
                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkLabel" id="sink_state">
                        <property name="margin-start">6</property>
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="label"></property>
                        <style>
                            <class name="dim-label" />
                        </style>
                    </object>
                </child>

                <child type="end">
                    <object class="GtkMenuButton" id="menubutton">
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="tooltip_text" translatable="yes">Settings</property>
                        <property name="icon_name">preferences-system-symbolic</property>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>